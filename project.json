{
  "name": "mcp-chain-of-draft-prompt-tool",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist",
        "main": "src/index.ts",
        "tsConfig": "tsconfig.json",
        "assets": ["package.json"]
      }
    },
    "sea": {
      "executor": "@nx-node-sea/plugin:build",
      "options": {
        "configPath": "sea-config.json"
      }
    }
  }
} 